{
  "parser": "babel-eslint",
  "env": {
		"browser": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["import"],
  "rules": {
    "strict": 0,
    "no-console": 0,
    "no-use-before-define": 0,
    "import/prefer-default-export": 0,
    "global-require": 0,
		"no-underscore-dangle": ["error", { "allow": ["_id"] }]
  }
}
